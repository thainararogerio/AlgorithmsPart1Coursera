package Quick_union;

import java.util.Arrays;

import Utils.StdOut;

public class UF {
	private int[] id;
	
	public UF(int N) {
		id = new int[N];
		
		for (int i = 0; i < id.length; i++) {
			id[i] = i;
		}
	}
	
	public void union(int p, int q) {
		
	}
	
	public boolean connected(int p, int q) {
		
	}

	@Override
	public String toString() {
		return "UF [id=" + Arrays.toString(id) + "]";
	}
	
	
}
